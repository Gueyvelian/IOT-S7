    {
        "id": "9d7e06f4c513c3e9",
        "type": "function",
        "z": "57a3c3f5c98dbf58",
        "name": "Moyenne de 5 points",
        "func": "console.log(\"ICI \" + msg.payload);\n\nlet arrayValues = flow.get(\"arrayValues\");\n\nlet value = msg.payload;\n\narrayValues.push(value);\n\nconsole.log(arrayValues);\n\nlet arrLength = arrayValues.length; // 20\nlet stop = arrLength - 5; // 15\n\nconsole.log(\"Longueur du tableau : \" + arrLength);\nconsole.log(\"Longueur du tableau - 5 : \" + stop);\n\nlet average = 0;\n\nif(arrLength >= 5)\n{\n    let sum = 0;\n    for (let i = arrLength - 1; i > stop; i--)\n    {\n        console.log(i);\n        console.log(arrayValues[i]);\n        sum += arrayValues[i];\n    }\n\n    average = sum/5;\n\n    console.log(average);\n\n    // Envoie au chart\n    msg.payload = average;\n\n    return msg;\n}\n\nflow.set(\"arrayValues\", arrayValues);",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "// Le code ajouté ici sera exécuté une fois\n// à chaque démarrage du noeud.\nflow.set(\"arrayValues\", new Array(0, 0, 0, 0));",
        "finalize": "",
        "libs": [],
