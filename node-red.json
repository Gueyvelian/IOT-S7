    {
        "id": "28c6beb802a83341",
        "type": "function",
        "z": "57a3c3f5c98dbf58",
        "name": "Couleur LED à allumer en fonction du SPO2/BPM",
        "func": "let bpm = msg.payload;\n\nlet ledToLight = \"\";\nif(bpm < 40)\n{\n    ledToLight = \"none\";\n}\nelse if(bpm < 85)\n{\n    ledToLight = \"green\";\n}\nelse if(bpm < 140)\n{\n    ledToLight = \"yellow\";\n}\nelse if(bpm > 140)\n{\n    ledToLight = \"red\";\n}\n\nmsg.payload = ledToLight;\n\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 1150,
